#!/bin/bash

rm /usr/bin/djv_convert
rm /usr/bin/djv_info
rm /usr/bin/djv_ls
rm /usr/bin/djv_view

if [ -x "$(command -v update-mime-database)" ]; then
    update-mime-database /usr/share/mime
fi
if [ -x "$(command -v gtk-update-icon-cache)" ]; then
    gtk-update-icon-cache /usr/share/icons/hicolor
fi

